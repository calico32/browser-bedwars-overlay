{
  "name": "bedwarsutil-server",
  "packageManager": "yarn@3.0.0",
  "scripts": {
    "build:dev": "NODE_ENV=development webpack",
    "watch": "NODE_ENV=development concurrently -c 'green,cyan' -n 'server,client' 'yarn watch:server' 'yarn watch:client'",
    "watch:server": "webpack watch",
    "watch:client": "webpack serve --config webpack.config.client.ts",
    "_dev": "NODE_ENV=development nodemon ./dist/server/main.js",
    "dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "build": "NODE_ENV=development concurrently -c 'green,cyan' -n 'server,client' 'yarn build:server' 'yarn build:client'",
    "build:server": "NODE_ENV=production webpack",
    "build:client": "NODE_ENV=production webpack --config webpack.config.client.ts",
    "prod": "NODE_ENV=production node ./dist/server/main.js",
    "postinstall": "rm -rf .yarn/.cache"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.19",
    "ws": "^8.0.0"
  },
  "resolutions": {
    "react-refresh": "0.10.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/concurrently": "^6",
    "@types/lodash": "^4",
    "@types/mini-css-extract-plugin": "^2",
    "@types/node": "^16.4.13",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/sass": "^1",
    "@types/source-map-support": "^0",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/webpack-dev-server": "^3",
    "@types/webpack-env": "^1.16.2",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "^10.3.1",
    "classnames": "^2.3.1",
    "concurrently": "^6.2.0",
    "css-loader": "^6.2.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "latest",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.1",
    "html-webpack-plugin": "^5.3.2",
    "hypixel-cache": "https://github.com/wiisportsresort/hypixel-cache.git",
    "hypixel-types": "https://github.com/wiisportsresort/hypixel-types.git#commit=cddb69ce1166744a72be48f56f03feb526bb852c",
    "mini-css-extract-plugin": "^2.2.0",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.12",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "tailwindcss": "^2.2.7",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
